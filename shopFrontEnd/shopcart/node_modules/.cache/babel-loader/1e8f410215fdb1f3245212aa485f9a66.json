D,GAFc;AAAA;;AAAA;AAE5B,4DAAmB;AAAA,cAAVH,CAAU;;AACjB,cAAIA,CAAC,CAACK,SAAN,EAAiB;AACf,gBAAIL,CAAC,CAACK,SAAF,CAAYrD,MAAZ,KAAuB,CAA3B,EAA8B;AAC5BgD,eAAC,CAACP,SAAF,GAAc,UAAd;AACF;AACF;AACF;AAR4B;AAAA;AAAA;AAAA;AAAA;;AAS5B,WAAKpD,KAAL,eAAkBR,EAAlB,GAAwB,CAAxB,EAA2B4D,SAA3B,GAAuC,gBAAvC;AACA,WAAKnE,OAAL,CAAa,KAAKG,OAAL,CAAamB,IAAb,CAAb,IAAmCf,EAAnC,CAV4B,CAW5B;;AACA,UAAIgB,MAAK,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,QAA1B,EAAoCC,MAAjD;AACA,UAAIC,QAAO,GAAIH,QAAQ,CAACC,gBAAT,CAA0B,QAA1B,EAAoCC,MAAnD;;AACA,UAAIH,MAAK,KAAMI,QAAf,EAAyB;AACvB,aAAKT,SAAL;;AACA,YAAI,KAAK8D,WAAL,CAAiBC,UAAjB,KAAgC,KAAKD,WAAL,CAAiBE,UAArD,EAAiE;AAC/D,eAAKC,QAAL;AACF;AACF;AACD,KAxKM;AAyKDA,YAzKC,sBAyKW;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACY,MAAI,CAACC,KAAL,CAAWtD,IAAX,CAAgB,eAAhB,EAAiC;AAAE9B,yBAAO,EAAE,MAAI,CAACA;AAAhB,iBAAjC,CADZ;;AAAA;AAAA;AACFgC,mBADE,yBACRjC,IADQ;AAEhB,sBAAI,CAACK,KAAL,GAAa4B,GAAG,CAACjC,IAAJ,CAASK,KAAtB;AACA,sBAAI,CAACH,OAAL,GAAe+B,GAAG,CAACjC,IAAJ,CAASQ,EAAxB;;AAHgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIlB;AA7KO;AA7BI,CAAf","names":["name","data","choices","priceId","undefined","mapping","price","address","text","id","aim","reflectAddress","sureAddress","number","props","methods","closeSuccess","$refs","SuccessPrompt","style","closeTips","category","tips","addTo","type","select","document","querySelectorAll","length","selected","judgeToken","$https","post","then","res","code","top","window","innerHeight","documentElement","scrollTop","setTimeout","$router","push","path","query","$route","num","zero","type_zero","one","type_one","two","type_two","three","type_three","four","type_four","five","type_five","six","type_six","catch","console","log","err","subNumber","sureChange","moveIn","className","ChoiceAddress","getAddress","moveOut","choiceAddress","detailAddress","parseInt","i","getOptPrice","index","lis","childNodes","classList","price_range","price__min","price__max","getPrice","$http"],"sourceRoot":"","sources":["/Users/yujunxiong/Desktop/shopFrontEnd/shopcart/src/components/detail/ShopCenter.vue"],"sourcesContent":["<template>\n  <div class=\"shop-center\">\n    <div class=\"title\">\n      {{title}}\n    </div>\n    <div id=\"pic\">\n      <div class=\"pic_container\">\n        <span style=\"padding-left: 10px;\">价格</span>\n        <i class=\"pic_num\" v-if=\"price_range\">\n          <span>￥</span>\n          <!--<i class=\"price\">1200.00{{price_range}}</i>-->\n          <i class=\"price\" v-if=\"price\">{{price}}.00</i>\n          <i class=\"price\" v-else-if=\"price_range.price__min===price_range.price__max\">{{price_range.price__min}}.00</i>\n          <i class=\"price\" v-else>{{price_range.price__min}}.00-{{price_range.price__max}}.00</i>\n        </i>\n        <a href=\"javascript:\">降价通知</a>\n      </div>\n      <span class=\"evaluate\">\n        <span>累计评价</span>\n        <i>{{evaluate}}</i>\n      </span>\n    </div>\n    <div class=\"place\">\n      <span style=\"padding-left: 10px;\">配送至</span>\n      <div @mouseover=\"moveIn\" @mouseout=\"moveOut\" id=\"address\" ref=\"address\">\n        {{sureAddress}}<i class=\"iconfont\" style=\"font-size: 16px;\">&#xe611;</i>\n      </div>\n      <div id=\"choice-address\" @mouseover=\"moveIn\" @mouseout=\"moveOut\" ref=\"ChoiceAddress\">\n        <ul id=\"ui-area-tab\">\n          <li v-for=\"item in address\" :key=\"item.id\" @click=\"choiceAddress(item.id)\" :class=\"[item.id===aim?'select-address':'']\">{{item.text}}<i class=\"iconfont\">&#xe611;</i></li>\n        </ul>\n        <ul id=\"ui-area-content-list\" ref=\"ShowAddress\" v-if=\"reflectAddress\">\n          <li v-for=\"item in reflectAddress\" :key=\"item.id\">\n            <a href=\"javascript:void(0)\" @click=\"detailAddress(item.id, item.type, item.name)\" :data-id=\"item.id\" :class=\"[aim===item.id?'current-address':'']\">{{item.name}}</a>\n          </li>\n        </ul>\n      </div>\n      <span style=\"padding: 0 15px 0 5px;font-weight: bold;\">有货</span>\n      <span>在线支付免运费<i class=\"iconfont\">&#xe68f;</i></span>\n      <span class=\"dd\">由 <i>京东</i> 发货, 并提供售后服务. 今日16:30前完成下单，预计3月16日19:00前发货</span>\n    </div>\n    <div class=\"se_su\">\n      <div class=\"se_su_one\">\n        <span style=\"margin-left: 10px;\">服务支持</span>\n        <div class=\"as_pu\"></div>\n        <span style=\"color: #005AA0;\">免费上门取退</span>\n        <span style=\"color: #005AA0;\">极速审核</span>\n      </div>\n      <div class=\"se_su_two\">\n        <span>可配送港澳特权保障</span>\n        <span>退换货运费险</span>\n        <span>自提</span>\n      </div>\n    </div>\n    <div class=\"category\" ref=\"category\">\n        <span class=\"tips\" ref=\"tips\">\n          请选择你要的商品信息\n          <span class=\"close-tips\" @click=\"closeTips\">x</span>\n        </span>\n        <div v-for=\"(item, index) in opt\" :key=\"index\" class=\"attr1\" :ref=\"`list${index}`\">\n          <span>{{item[0].title}}</span>\n          <ul class=\"ca_ch\">\n            <li v-for=\"value in item[1].content\"\n                :key=\"value.id\"\n                class=\"a-choice\"\n                @click=\"getOptPrice(value.id, index, value.type)\"\n                :ref=\"`list${value.id}`\">\n              <img :src=\"value.image\" alt=\"\" v-show=\"value.image\"/>\n              <i>{{value.content}}</i>\n            </li>\n          </ul>\n        </div>\n    </div>\n    <div class=\"quantity\">\n        <span>数量</span>\n        <span class=\"calculation\">\n            <button @click=\"subNumber\">-</button>\n            <!--<input type=\"text\" v-model=\"number\" @keyup=\"changeNumber\" @blur=\"sureChange\"/>-->\n            <input v-model=\"number\" type=\"number\" autocomplete=\"off\" @blur=\"sureChange\" onkeypress=\"return (/[\\d]/.test(String.fromCharCode(event.keyCode)))\" style=\"ime-mode:Disabled\">\n            <button @click=\"this.number++\">+</button>\n        </span>\n    </div>\n    <div class=\"satis\">\n        <a class=\"purchase\" href=\"javascript:void(0)\" @click=\"addTo(0)\">立即购买</a>\n        <button class=\"collection\" @click=\"addTo(1)\"><i class=\"iconfont\" style=\"margin-right: 10px;\">&#xe63f;</i>加入购物车</button>\n    </div>\n    <!-- 成功加入购物车提示 -->\n    <div class=\"success-prompt\" ref=\"SuccessPrompt\">\n      <img src=\"@/assets/verygoods/sucess.png\" alt=\"\" />\n      <div>\n          <h5>成功加入购物车</h5>\n          <span>你可以去购物车结算，或先使用 天猫APP 领取新用户福利礼包立即下载天猫APP</span>\n      </div>\n      <span class=\"close-s-p\" @click=\"closeSuccess\">X</span>\n    </div>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'shopCenter',\n  data () {\n    return {\n      choices: {},\n      priceId: undefined,\n      mapping: {\n        0: 'type_zero',\n        1: 'type_one',\n        2: 'type_two',\n        3: 'type_three',\n        4: 'type_four',\n        5: 'type_five',\n        6: 'type_six'\n      },\n      price: '',\n      address: {\n        0: { text: '四川省', id: 51 },\n        1: { text: '成都市', id: 5101 },\n        2: { text: '双流区', id: 510116 },\n        3: { text: '万安街道', id: 510116018 }\n      },\n      aim: 510116018,\n      reflectAddress: '',\n      sureAddress: '四川省成都市双流区万安街道',\n      number: 1\n    }\n  },\n  props: ['opt', 'title', 'price_range', 'evaluate'],\n  methods: {\n    closeSuccess () {\n      this.$refs.SuccessPrompt.style = 'display: none'\n    },\n    // 关闭提示\n    closeTips () {\n      this.$refs.category.style = 'border: 0'\n      this.$refs.tips.style = 'display: none'\n    },\n    // 立即购买和收藏\n    addTo (type) {\n      let select = document.querySelectorAll('.ch-re').length\n      let selected = document.querySelectorAll('.attr1').length\n      if (select === selected) {\n        // 加入购物车\n        if (type) {\n          this.judgeToken(1)\n        } else {\n          // 购买\n          this.judgeToken(0)\n        }\n      } else {\n        this.$refs.category.style = 'border: 1px solid red'\n        this.$refs.tips.style = 'display: inline-block'\n      }\n    },\n    // 验证token的有效性 并进行购买或收藏\n    judgeToken (type) {\n      this.$https.post('store/obtainShop/', { type: type, choices: this.choices, number: this.number }).then(res => {\n        // 加入购物车\n        if (res.data.code === 200) {\n          // 加入购物车成功\n          if (type) {\n            this.$refs.SuccessPrompt.style = 'display: flex'\n            this.$refs.SuccessPrompt.style.top = window.innerHeight / 2 + document.documentElement.scrollTop + 'px'\n            setTimeout(() => {\n              this.$refs.SuccessPrompt.style = 'display: none'\n            }, 3000)\n            // 跳转去购买页\n          } else {\n            this.$router.push({\n              path: '/payment',\n              query: {\n                id: this.$route.query.id,\n                priceId: this.priceId,\n                num: this.number,\n                zero: this.choices.type_zero,\n                one: this.choices.type_one,\n                two: this.choices.type_two,\n                three: this.choices.type_three,\n                four: this.choices.type_four,\n                five: this.choices.type_five,\n                six: this.choices.type_six\n              }\n            })\n            // 跳转打开新的页面\n            /* const newpage = this.$router.resolve({\n              path: '/payment', // 跳转的页面路由\n              query: { // 要传的参数\n                id: this.$route.query.id,\n                priceId: this.priceId,\n                num: this.number,\n                zero: this.choices.type_zero,\n                one: this.choices.type_one,\n                two: this.choices.type_two,\n                three: this.choices.type_three,\n                four: this.choices.type_four,\n                five: this.choices.type_five,\n                six: this.choices.type_six\n              }\n            })\n            window.open(newpage.href, '_blank') */\n          }\n          // 登录提示或者请求失败提示\n        } else {\n\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    subNumber () {\n      if (this.number > 1) {\n        this.number--\n      }\n    },\n    /* // 商品数量的约束\n    changeNumber () {\n      const reg = /^[1-9][0-9]*$/\n      let result = reg.test(this.number)\n      if (!result) {\n        this.number = this.number.substr(0, this.number.length - 1)\n      }\n    }, */\n    sureChange () {\n      if (!this.number) {\n        this.number = 1\n      }\n    },\n    // 移入\n    moveIn () {\n      this.$refs.address.className = 'address-hover'\n      this.$refs.ChoiceAddress.style = 'display: table'\n      if (!this.reflectAddress) {\n        this.getAddress(this.address[3].id)\n      }\n    },\n    // 移出\n    moveOut () {\n      this.$refs.address.className = ''\n      this.$refs.ChoiceAddress.style = 'none'\n    },\n    // 选择 省， 市， 区， 街\n    choiceAddress (id) {\n      this.aim = id\n      // 发送 axios 请求\n      this.getAddress(id)\n    },\n    // 详细地址请求\n    detailAddress (id, type, name) {\n      this.address[parseInt(type)] = { text: name, id: id }\n      if (type !== 3) {\n        for (let i of [0, 1, 2, 3]) {\n          if (i > parseInt(type)) {\n            delete this.address[i]\n          }\n        }\n        this.address[parseInt(type) + 1] = { text: '请选择', id: 0 }\n        this.aim = 0\n        this.getAddress(id, 1)\n      } else {\n        this.aim = id\n        this.moveOut()\n        // 将选择好的值赋值给选择框\n        this.sureAddress = ''\n        for (let i of [0, 1, 2, 3]) {\n          this.sureAddress += this.address[i].text\n        }\n      }\n    },\n    // 发送axios请求获取地址选择数据数据\n    getAddress (id, type) {\n      this.$https.post('store/address/', { id: id, type: type }).then(res => {\n        this.reflectAddress = res.data.data.address\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    getOptPrice (id, index, type) {\n      let lis = this.$refs[`list${index}`][0].childNodes[1].childNodes\n      for (let i of lis) {\n        if (i.classList) {\n          if (i.classList.length === 2) {\n            i.className = 'a-choice'\n          }\n        }\n      }\n      this.$refs[`list${id}`][0].className = 'ch-re a-choice'\n      this.choices[this.mapping[type]] = id\n      // 需要判断当前商品是否有多个价格\n      let select = document.querySelectorAll('.ch-re').length\n      let selected = document.querySelectorAll('.attr1').length\n      if (select === selected) {\n        this.closeTips()\n        if (this.price_range.price__min !== this.price_range.price__max) {\n          this.getPrice()\n        }\n      }\n    },\n    async getPrice () {\n      const { data: res } = await this.$http.post('store/update/', { choices: this.choices })\n      this.price = res.data.price\n      this.priceId = res.data.id\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n.shop-center {\n  width: 590px;\n  .title {\n    font: 700 16px Arial,\"microsoft yahei\";\n    color: #666;\n    overflow: hidden;\n    width: 590px;\n    line-height: 28px;\n    padding-top: 10px;\n    margin-bottom: 10px;\n    box-sizing: content-box;\n    text-align: left;\n  }\n}\n#pic {\n  width: 590px;\n  height: 35px;\n  padding-top: 10px;\n  box-sizing: content-box;\n  font: 12px/150% tahoma,arial,Microsoft YaHei,Hiragino Sans GB,\"\\u5b8b\\u4f53\",sans-serif;\n  color: #999;\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  a {\n    text-decoration: none;\n    color: #005AA0;\n  }\n  i {\n    font-style: normal;\n  }\n}\n.pic_num {\n  color: #E4393C;\n  font-size: 22px;\n  margin: 0 25px 0 15px;\n  span {\n    font-size: 16px;\n  }\n}\n\n.evaluate {\n  display: flex;\n  flex-direction: column;\n  border-left: 1px solid #E6E6E6;\n  width: 48px;\n  height: 32px;\n  box-sizing: content-box;\n  padding: 0 10px;\n  align-items: center;\n  justify-content: center;\n  i {\n    font: 14px verdana;\n    color: #005ea7;\n  }\n}\n\n.pic_container {\n    display: flex;\n    align-items: center;\n    justify-content: flex-start;\n}\n\n.place {\n  display: flex;\n  font-size: 12px;\n  color: #999;\n  align-items: center;\n  margin-top: 10px;\n  flex-wrap: 