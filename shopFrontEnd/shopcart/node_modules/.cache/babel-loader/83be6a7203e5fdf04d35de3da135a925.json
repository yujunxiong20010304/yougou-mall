CD,KAHM;AAIDC,mBAJC,6BAIkB;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACK,KAAI,CAACC,KAAL,CAAWC,IAAX,CAAgB,eAAhB,EAAiC;AAAEC,oBAAE,EAAE,KAAI,CAACC,MAAL,CAAYC,KAAZ,CAAkBF;AAAxB,iBAAjC,CADL;;AAAA;AAAA;AACTG,mBADS,yBACfZ,IADe;AAEvB,qBAAI,CAACC,YAAL,GAAoBW,GAAG,CAACZ,IAAxB;;AAFuB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGzB;AAPO,GAtBI;AA+Bba,SA/Ba,qBA+BF;AACT,SAAKP,eAAL;AACF;AAjCa,CAAf","names":["titles","ShopLeft","ShopCenter","ShopRight","ShopRecommend","ShopIntroduce","ShopParameter","ImageParameter","packing","guarantee","discuss","similar","name","components","data","goods_detail","aim","methods","changeDisplay","value","goodsDetailInfo","$http","post","id","$route","query","res","created"],"sourceRoot":"","sources":["/Users/yujunxiong/Desktop/shopFrontEnd/shopcart/src/views/detail/Detail.vue"],"sourcesContent":["<template>\n  <div id=\"detail\">\n    <all-header></all-header>\n    <all-search></all-search>\n    <shop-title></shop-title>\n    <div id=\"shop-info\">\n      <shop-left :show_images=\"goods_detail.show_images\"></shop-left>\n      <shop-center\n        :opt=\"goods_detail.opt\"\n        :title=\"goods_detail.title\"\n        :price_range=\"goods_detail.price_range\"\n        :evaluate=\"goods_detail.evaluate\"\n      >\n      </shop-center>\n      <shop-right :shops=\"goods_detail.shops\"></shop-right>\n    </div>\n    <shop-recommend :shops=\"goods_detail.shops\"></shop-recommend>\n    <div id=\"introduce\">\n      <shop-introduce :shops=\"goods_detail.shops\"></shop-introduce>\n      <div id=\"suggest\">\n        <div class=\"detail-title\">\n          <ul>\n            <li @click=\"changeDisplay(1)\" :class=\"[1 === aim?'title-select':'']\">商品介绍</li>\n            <li @click=\"changeDisplay(2)\" :class=\"[2 === aim?'title-select':'']\">规格与包装</li>\n            <li @click=\"changeDisplay(3)\" :class=\"[3 === aim?'title-select':'']\">售后保障</li>\n            <li @click=\"changeDisplay(4)\" :class=\"[4 === aim?'title-select':'']\">商品评价(1000+)</li>\n            <li @click=\"changeDisplay(5)\" :class=\"[5 === aim?'title-select':'']\">类似好评商品</li>\n          </ul>\n          <span>加入购物车</span>\n        </div>\n        <shop-parameter :style=\"[1 < aim?'display:none':'display:block']\" :details_args=\"goods_detail.details_args\"></shop-parameter>\n        <packing :style=\"[2 === aim?'display:flex':'display:none']\"></packing>\n        <image-parameter :style=\"[1 < aim?'display:none':'display:block']\" :image_args=\"goods_detail.image_args\"></image-parameter>\n        <guarantee :style=\"[3 < aim?'display:none':'display:block']\"></guarantee>\n        <discuss :style=\"[4 < aim?'display:none':'display:block']\"></discuss>\n        <similar :shops=\"goods_detail.shops\"></similar>\n      </div>\n    </div>\n    <all-tail></all-tail>\n  </div>\n</template>\n\n<script>\nimport titles from '@/components/detail/titles'\nimport ShopLeft from '@/components/detail/ShopLeft'\nimport ShopCenter from '@/components/detail/ShopCenter'\nimport ShopRight from '@/components/detail/ShopRight'\nimport ShopRecommend from '@/components/detail/ShopRecommend'\nimport ShopIntroduce from '@/components/detail/ShopIntroduce'\nimport ShopParameter from '@/components/detail/ShopParameter'\nimport ImageParameter from '@/components/detail/ImageParameter'\nimport packing from '@/components/detail/packing'\nimport guarantee from '@/components/detail/guarantee'\nimport discuss from '@/components/detail/discuss'\nimport similar from '@/components/detail/similar'\nexport default {\n  name: 'Detail',\n  components: {\n    'shop-title': titles,\n    'shop-left': ShopLeft,\n    'shop-center': ShopCenter,\n    'shop-right': ShopRight,\n    'shop-recommend': ShopRecommend,\n    'shop-introduce': ShopIntroduce,\n    'shop-parameter': ShopParameter,\n    'image-parameter': ImageParameter,\n    packing: packing,\n    guarantee: guarantee,\n    discuss: discuss,\n    similar: similar\n  },\n  data () {\n    return {\n      goods_detail: {},\n      aim: 1\n    }\n  },\n  methods: {\n    changeDisplay (value) {\n      this.aim = value\n    },\n    async goodsDetailInfo () {\n      const { data: res } = await this.$http.post('store/detail/', { id: this.$route.query.id })\n      this.goods_detail = res.data\n    }\n  },\n  created () {\n    this.goodsDetailInfo()\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n#detail {\n  background-color: #F2F2F2;\n}\n#shop-info {\n  display: flex;\n  width: 1210px;\n  justify-content: space-between;\n  margin: 25px auto 0;\n}\n#introduce {\n  display: flex;\n  width: 1210px;\n  margin: 0 auto;\n  justify-content: space-between;\n}\n#suggest {\n  width: 988px;\n}\n.detail-title {\n  width: 988px;\n  height: 38px;\n  border-bottom: 1px solid #e4393c;\n  background-color: #F7F7F7;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 5px;\n  span {\n    width: 92px;\n    height: 25px;\n    background-color: #DF3133;\n    font-size: 14px;\n    color: #fff;\n    text-align: center;\n    line-height: 25px;\n    margin-right: 15px;\n    cursor: pointer;\n  }\n  ul {\n    display: flex;\n    color: #fff;\n    font-size: 14px;\n    font-family: \"microsoft yahei\";\n    padding: 0;\n    margin: 0;\n    height: 100%;\n    li {\n      padding: 10px 25px;\n      margin-right: 2px;\n      cursor: pointer;\n      font-size: 14px;\n      color: #666;\n    }\n    .title-select {\n      background-color: #e4393c;\n      color: #fff;\n    }\n  }\n}\n</style>\n"]}]}