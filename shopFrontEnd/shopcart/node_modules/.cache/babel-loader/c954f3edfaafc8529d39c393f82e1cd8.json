=\"#\">\n                    <img :src=\"user_image?user_image:'https://w.wallhaven.cc/full/e7/wallhaven-e7lomr.jpg'\" alt=\"\" />\n                </a>\n                <a href=\"#\">\n                    <span>Hi!</span>\n                    <span style=\"font-weight: bold;\">{{username?username:'你好'}}</span>\n                </a>\n                <ul v-if=\"shop_info\">\n                  <li v-for=\"(item, index) in shop_info\" :key=\"index\">\n                      <a href=\"#\">\n                          <span>{{item.num}}</span><i>{{item.status}}</i>\n                      </a>\n                  </li>\n                </ul>\n                <div v-else class=\"member-ft\">\n                  <div>登录</div>\n                  <div>注册</div>\n                  <div>开店</div>\n                </div>\n            </div>\n            <!--{# 轮播图 #}-->\n            <div class=\"merry_go_round\">\n                <div id=\"carouselExampleSlidesOnly\" class=\"carousel slide\" data-bs-ride=\"carousel\">\n                  <div class=\"carousel-inner\">\n                    <div class=\"carousel-item active\">\n                      <img src=\"@/assets/backImage/five.jpeg\" class=\"d-block w-100\" alt=\"...\">\n                    </div>\n                    <div class=\"carousel-item\">\n                      <img src=\"@/assets/backImage/six.jpeg\" class=\"d-block w-100\" alt=\"...\">\n                    </div>\n                  </div>\n                </div>\n            </div>\n            <!--{# 新闻导航 #}-->\n            <div id=\"notice\">\n                <div class=\"times\">\n                  <span>{{hours}}</span>:<span>{{minute}}</span>\n                </div>\n                <video autoplay loop muted>\n                  <source src=\"@/assets/backImage/test.mp4\" type=\"video/mp4\" />\n                </video>\n            </div>\n            <!--{# 底部导航 #}-->\n            <div id=\"bottom_nav\">\n                <a href=\"#\">\n                    <i class=\"bot iconfont\">&#xe733;</i>\n                    <span>收藏的宝贝</span>\n                </a>\n                <a href=\"#\">\n                    <i class=\"bot iconfont\">&#xe8bb;</i>\n                    <span>买过的店铺</span>\n                </a>\n                <a href=\"#\">\n                    <i class=\"bot iconfont\">&#xe895;</i>\n                    <span>收藏的店铺</span>\n                </a>\n                <a href=\"#\">\n                    <i class=\"bot iconfont\">&#xe662;</i>\n                    <span>我的足迹</span>\n                </a>\n            </div>\n        </div>\n    </article>\n</template>\n\n<script>\nexport default {\n  name: 'navigation',\n  data () {\n    return {\n      year: '',\n      month: '',\n      date: '',\n      day: '',\n      minute: '',\n      hours: '',\n      shop_info: ''\n    }\n  },\n  computed: {\n    username () {\n      return this.$store.state.username\n    },\n    user_image () {\n      return this.$store.state.user_image\n    }\n  },\n  methods: {\n    getRotationPage (e) {\n      const index = e.target.querySelector('.active').getAttribute('data-index')\n      this.$refs.index.innerHTML = index\n    },\n    getTime () {\n      /* 指定时间 */\n      let nowdate = new Date()\n      this.year = nowdate.getFullYear() // 年\n      this.month = nowdate.getMonth() + 1 // 月\n      this.date = nowdate.getDate() // 号\n      this.day = nowdate.getDay() // 星期\n      this.minute = nowdate.getMinutes() // 分\n      this.hours = nowdate.getHours()\n    },\n    // 获取用户购物信息\n    getShoppingInfo () {\n      this.$https.post('store/shopping/').then(res => {\n        if (res.data.code === 200) {\n          this.shop_info = res.data.data.shop_info\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    }\n  },\n  created () {\n    this.getShoppingInfo()\n  },\n  mounted () {\n    setTimeout(() => {\n      this.getTime()\n    })\n    setInterval(() => {\n      this.getTime()\n    }, 6000)\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n\n#side_nav {\n  width: 1155px;\n  height: 575px;\n  display: flex;\n  justify-content: space-between;\n  background-color: #fff;\n  padding: 25px 0;\n  border-radius: 15px;\n  margin: 0 auto 45px;\n  #bulletin_board {\n    color: #666666;\n    font-size: 14px;\n    height: 510px;\n    width: 194px;\n    ul {\n      display: flex;\n      flex-direction: column;\n      justify-content: space-around;\n      height: 510px;\n      width: 194px;\n      margin: 6px 0 3px 18px;\n      padding: 0;\n      .font {\n        font-size:16px;\n        margin-right: 10px;\n      }\n      li {\n        height: 20px;\n        line-height: 20px;\n        text-align: left;\n        &:hover .h{\n          color: #FF3701;\n        }\n        a {\n          color: #666666;\n          font-size: 14px;\n          text-decoration: none;\n          &:hover {\n            text-decoration: underline;\n          }\n        }\n        span {\n          padding: 0 3px;\n        }\n      }\n    }\n  }\n}\n\n#central_navigation {\n  width: 520px;\n  height: 512px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n\n  #central_navigation_one {\n    min-width: 520px;\n    height: 280px;\n    border-radius: 15px;\n    overflow: hidden;\n    margin-top: 6px;\n    position: relative;\n    &:hover button{\n      display: block;\n    }\n  }\n  #central_navigation_two {\n    min-width: 520px;\n    height: 201px;\n    border-radius: 15px;\n    position: relative;\n    &:hover button{\n      display: block;\n    }\n    img {\n      width: 251px;\n      height: 190px;\n      border-radius: 15px;\n    }\n    .carousel-caption {\n      right: 0;\n      left: 0;\n      padding: 0;\n      width: 520px;\n      height: 201px;\n      bottom: 0;\n    }\n    .seize_a_seat {\n      min-width: 520px;\n      height: 201px;\n      display: flex;\n      justify-content: space-around;\n      align-items: center;\n      background-color: #fff;\n      border-radius: 15px;\n    }\n  }\n}\n\n#central_navigation_one button,  #central_navigation_two button{\n    height: 30px;\n    width: 20px;\n    position: absolute;\n    top: 50%;\n    transform: translate(0, -50%);\n    background-color: rgba(0,0,0,.3);\n    margin: 0;\n    display: none;\n}\n\n.btn_one {\n    border-bottom-right-radius: 15px;\n    border-top-right-radius: 15px;\n    text-align: left;\n}\n.btn_two {\n    border-bottom-left-radius: 15px;\n    border-top-left-radius: 15px;\n    text-align: right;\n}\n#current_page {\n  text-align: left;\n  height:24px;\n  width: 100%;\n  margin-top: 10px;\n  span {\n    color: #FF0136;\n    font-weight: bold;\n    margin-right: 5px;\n  }\n  i:last-child {\n    float: right;\n    color: #3c3c3c;\n  }\n  i {\n    font-style: normal;\n    font-size: 12px;\n    color: #999;\n    line-height: 24px;\n  }\n  em {\n    font-style: normal;\n    color: #FF1648;\n  }\n}\n\n#user_information {\n    background-color: #E5E5E5;\n    width: 384px;\n    height: 508px;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-around;\n    border-radius: 15px;\n}\n.user_information_head {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 15px;\n  .member-ft {\n    display: flex;\n    justify-content: space-between;\n    width: 356px;\n    margin-top: 10px;\n    div {\n      color: #fff;\n      background-color: #E1251B;\n      width: 105px;\n      height: 32px;\n      line-height: 32px;\n      text-align: center;\n      border-radius: 7px;\n      cursor: pointer;\n      font-size: 14px;\n    }\n  }\n  img {\n    width: 50px;\n    height: 50px;\n    border-radius: 50%;\n    border: 1px solid #FABF9E;\n  }\n  a {\n    text-decoration: none;\n    span {\n      font-size: 12px;\n      color: #3c3c3c;\n    }\n  }\n  ul {\n    display: flex;\n    align-items: center;\n    justify-content: space-around;\n    padding-left: 14px;\n    width: 90%;\n    margin-top: 10px;\n  }\n  li {\n    a {\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      justify-content: center;\n      &:hover i{\n        color: #f40;\n      }\n    }\n    i {\n      font-size: 12px;\n      color: #3c3c3c;\n      font-style: normal;\n    }\n    span {\n      font-size: 14px;\n      color: #f40;\n      font-weight: bold;\n    }\n  }\n\n}\n.merry_go_round {\n  width: 356px;\n  height: 141px;\n  border-radius: 5px;\n  overflow: hidden;\n  margin: 0 auto;\n}\n#notice {\n  height: 125px;\n  padding: 15px;\n  position: relative;\n  overflow: hidden;\n  &:hover video{\n    filter: blur(1px); //背景模糊设置\n  }\n  .times {\n    z-index: 999;\n    position: absolute;\n    left: 50%;\n    top: 50%;\n    transform: translate(-50%, -50%);\n    color: #fff;\n    font-size: 40px;\n  }\n  video {\n    width: 355px;\n    position: absolute;\n    z-index: 1;\n    left: 50%;\n    top: 50%;\n    transform: translate(-50%, -50%);\n    border-radius: 15px;\n    // -webkit-filter: grayscale(100%);\n    // filter:grayscale(100%); //背景灰度设置*/\n  }\n  //video {\n  //  position: fixed;\n  //  right: 0px;\n  //  bottom: 0px;\n  //  min-width: 100%;\n  //  min-height: 100%;\n  //  height: auto;\n  //  width: auto;\n  //  z-index: -1;\n  //  source{\n  //    min-width: 100%;\n  //    min-height: 100%;\n  //    height: auto;\n  //    width: auto;\n  //  }\n  //}\n}\n\n#bottom_nav {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-left: 12px;\n  margin-bottom: 15px;\n  a {\n    text-decoration: none;\n    font-size: 12px;\n    color: #333;\n    display: flex;\n    width: 60px;\n    height: 53px;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    i {\n      font-size: 21px;\n    }\n    &:hover i{\n      color: #FF6E07;\n    }\n  }\n}\n</style>\n"]}]}