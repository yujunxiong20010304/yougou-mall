 heads,\n    'scan-code': ScanCode,\n    payoff: payoff,\n    ending: ending\n  },\n  methods: {\n    getShopCart () {\n      this.$https.post('payment/orderInfo/', { id: this.$route.query.id }).then(res => {\n        console.log(res)\n        if (res.data.code === 200) {\n          this.store = res.data.data.result[0].price_id__goods_id__store\n          this.title = res.data.data.result[0].price_id__goods_id__title\n          this.price = res.data.data.result[0].price_id__price\n          this.num = res.data.data.result[0].num\n          this.balance = res.data.data.result[0].user_id__balance\n          this.username = res.data.data.result[0].user_id__username\n          this.id = res.data.data.result[0].id\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    }\n  },\n  created () {\n    this.getShopCart()\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n#payment {\n  background-color: #EFF0F1;\n}\n</style>\n"]}]}