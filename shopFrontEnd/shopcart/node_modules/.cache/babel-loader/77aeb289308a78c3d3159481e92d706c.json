 0 })\n      if (res.code === 200) {\n        this.goods_info = res.data\n      }\n    }\n  },\n  created () {\n    this.getGoodsInfo('数码')\n    emitter.on('recommend', (e) => {\n      this.aim = e.aim\n    })\n  },\n  watch: {\n    aim: {\n      handler (newVal, oldVal) {\n        let theme = this.recommend[newVal]\n        this.getGoodsInfo(theme)\n      },\n      immediate: true\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n#home {\n  background-color: #fff;\n}\n</style>\n"]}]}