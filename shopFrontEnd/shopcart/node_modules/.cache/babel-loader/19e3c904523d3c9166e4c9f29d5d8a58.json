    <el-menu-item index=\"2-2\" @click=\"change\">收货信息</el-menu-item>\n      </el-sub-menu>\n\n      <el-sub-menu index=\"3\">\n        <template #title>\n          <el-icon class=\"iconfont\">&#xe697;</el-icon>\n          <span>我的关注</span>\n        </template>\n          <el-menu-item index=\"3-1\" @click=\"change\">关注店铺</el-menu-item>\n          <el-menu-item index=\"3-2\" @click=\"change\">关注商品</el-menu-item>\n      </el-sub-menu>\n    </el-menu>\n  </div>\n  <!--<el-radio-group v-model=\"isCollapse\" style=\"margin-bottom: 20px\">\n    &lt;!&ndash;<el-radio-button :label=\"false\">expand</el-radio-button>&ndash;&gt;\n    <el-radio-button :label=\"true\"><el-icon class=\"iconfont\">&#xe693;</el-icon></el-radio-button>\n  </el-radio-group>-->\n</template>\n<script>\nimport emitter from '@/utils/event'\n\nexport default {\n  name: 'sidebar',\n  data () {\n    return {\n      isCollapse: true,\n      height: ''\n    }\n  },\n  methods: {\n    change (name) {\n      emitter.emit('change', { aim: name })\n    },\n    handleOpen (key, keyPath) {\n      console.log(key, keyPath)\n    },\n    handleClose (key, keyPath) {\n      console.log(key, keyPath)\n    }\n  },\n  mounted () {\n    emitter.on('stretch', (e) => {\n      this.isCollapse = e.aim\n    })\n    // 设置窗口大小\n    this.height = document.documentElement.clientHeight\n    window.onresize = () => {\n      return (() => {\n        this.height = document.documentElement.clientHeight\n      })()\n    }\n  },\n  watch: {\n    height: {\n      handler (newVal, oldVal) {\n        emitter.emit('height', { aim: this.height })\n      }\n    }\n  }\n}\n</script>\n\n<style>\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 200px;\n  height: 100%;\n}\n.el-menu--collapse {\n  height: 100%;\n}\n</style>\n"]}]}