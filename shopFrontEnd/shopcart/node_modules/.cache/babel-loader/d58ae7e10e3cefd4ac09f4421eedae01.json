+J,IAA/J,EAA+JC,YAA5IC,oBAA4I,EAA9H,UAAtBC,IAAsB,EAAlB;yBAAfL,oBAA+J,IAA/J,EAA+J;AAA3H,iBAASK,IAAI,CAACb,EAA6G;AAAxGgB,aAAK;AAAA,eAAEC,uBAAcJ,IAAI,CAACb,EAAnB,EAAuBa,IAAI,CAACsB,IAA5B,CAAF;AAAA,OAAmG;AAA7DjC,WAAK,mBAAGW,IAAI,CAACb,EAAL,KAAUoC,QAAQ,CAACxB,UAAD,CAAlB,GAAuB,IAAvB,GAAuB,EAA1B;AAAwD,KAA/J,mBAA+IC,IAAI,CAACsB,KAApJ,EAAwJ;AAAA;AAAxJ,MAAwJE,WAAxJ;GAA+J,CAA/J;;AAAA,GACG,EAFL,CAGI,CAbN;;AAAA,GAcI,CAtBN,CAyCI,EAlBJC,WAkBI,CAhDN,EADF,CAmDK,CAxDP;;AAAA,GA0DI,EADJrC,oBAA+D,KAA/D,EAA+D;AAA1DC,SAAK,EAAC,MAAoD;AAA7CI,OAAG,EAAC,MAAyC;AAAjCU,WAAK;AAAA,aAAOC,2EAAP;AAAA,OAAoB,QAApB;AAA4B,GAA/D;;AAAA,GACI,CAlGN","names":["_imports_0","_imports_1","id","_createElementVNode","class","href","src","alt","ref","style","_createElementBlock","_hoisted_2","_Fragment","_renderList","$data","item","index","key","onClick","$options","onMouseover","onMouseout","_toDisplayString","city_address","receiver","detailed_address","phone","_hoisted_7","_hoisted_11","_hoisted_13","_createCommentVNode","_hoisted_16","_hoisted_18","_hoisted_19","_hoisted_21","_hoisted_24","_hoisted_26","name","parseInt","_hoisted_28","_hoisted_29"],"sourceRoot":"","sources":["/Users/yujunxiong/Desktop/shopFrontEnd/shopcart/src/components/payment/place.vue"],"sourcesContent":["<template>\n  <div>\n    <figure id=\"receive-selection\">\n        <h6>选择收货地址</h6>\n        <div class=\"address-list\">\n          <div\n            :class=\"[aim===index?'inner-infos sa':'inner-infos']\"\n            v-for=\"(item, index) in test_receiving\"\n            :key=\"item.id\"\n            @click=\"selectAddress(index)\"\n            @mouseover=\"moveIn(index)\"\n            @mouseout=\"moveOut\"\n            :style=\"[index>truncation?'display:none':'display:block']\"\n          >\n            <div class=\"addr-hd\">\n              <span>{{item.city_address[0][0]}}</span>\n              <span>{{item.city_address[1][0]}}</span>\n              <span>({{item.receiver}})</span>\n            </div>\n            <div class=\"addr-bd\">\n              <span>{{item.city_address[2][0]}}</span>\n              <span>{{item.city_address[3][0]}}</span>\n              <span>{{item.detailed_address}}</span>\n              <span>{{item.phone}}</span>\n            </div>\n            <a href=\"javascript:\">修改</a>\n            <img src=\"@/assets/verygoods/fencec.png\" alt=\"\" v-if=\"aim===index||plan===index\"/>\n            <img src=\"@/assets/verygoods/fence.png\" alt=\"\" v-else/>\n            <span class=\"dt\" v-if=\"index===0\">默认地址</span>\n            <div class=\"selected\">\n              <span class=\"iconfont\">&#xe650;</span>\n            </div>\n          </div>\n        </div>\n        <div class=\"operations\">\n            <button :style=\"[address_quantity>4?'display:none':'display:block']\" @click=\"addAddress\">使用新地址</button>\n            <a href=\"javascript:\" :style=\"[address_quantity>4?'display:block':'display:none']\" @click=\"showAllAddress\">显示全部地址</a>\n            <a href=\"javascript:\">管理收货地址</a>\n        </div>\n    </figure>\n    <!-- 可丢弃 -->\n    <main class=\"add-address\" ref=\"addAddress\">\n      <div class=\"next-dialog-header\">\n        <span>新建地址</span>\n        <i style=\"cursor: pointer;\" class=\"iconfont\" @click=\"closeAddAddress\">&#xe740;</i>\n      </div>\n      <div class=\"address-container\" @click.self=\"cancelAddress\">\n        <div class=\"page-buy\">\n          <span class=\"headTips\">新增收货地址</span>\n          <div class=\"region\">\n              <span>当前配送至</span>\n              <span>中国大陆</span>\n              <span>切换<i class=\"iconfont\">&#xe62e;</i></span>\n          </div>\n          <div class=\"address-choices\">\n            <span class=\"currency-head\"><i>*</i>地址信息：</span>\n            <div class=\"tips\" @click=\"choiceAddress\">\n              <span class=\"address\" v-if=\"text\" style=\"color: black;\">{{text}}</span>\n              <span class=\"address\" v-else>请选择省/市/区/街道</span>\n              <i class=\"iconfont\">&#xe62b;</i>\n            </div>\n            <!-- 地址选择 -->\n            <div class=\"choice\" ref=\"choice\">\n              <ul>\n                <li\n                  v-for=\"(item, index)\n                  in choice_address\"\n                  @click=\"choices(index)\"\n                  :ref=\"`address${index}`\"\n                  :class=\"[index===goal?'aim':'']\"\n                >{{item}}</li>\n              </ul>\n              <ul class=\"address-pack\">\n                <li v-for=\"item in detail_address\" :data-id=\"item.id\" @click=\"detailAddress(item.id, item.name)\" :class=\"[item.id===parseInt(sign)?'yj':'']\">{{item.name}}</li>\n              </ul>\n            </div>\n          </div>\n          <div class=\"detail-address\">\n              <span class=\"currency-head\"><i>*</i>详细地址：</span>\n              <textarea placeholder=\"请输入详细地址信息，如道路、门牌号、小区、楼栋号、单元等信息\" rows=\"2\" cols=\"2\"></textarea>\n          </div>\n          <div class=\"postal-code\">\n              <span class=\"currency-head\"><i style=\"opacity: 0;\">*</i>邮政编码：</span>\n              <input class=\"currency-into\" type=\"text\" placeholder=\"请填写邮编\"/>\n          </div>\n          <div class=\"username\">\n              <span class=\"currency-head\"><i>*</i>收货人姓名：</span>\n              <input class=\"currency-into\" type=\"text\" placeholder=\"长度不超过25个字符\"/>\n          </div>\n          <div class=\"phone\">\n              <span class=\"currency-head\"><i>*</i>手机号：</span>\n              <div>中国大陆 +86<i class=\"iconfont\">&#xe62b;</i></div>\n              <input type=\"text\" placeholder=\"电话号码、手机号码必须填一项\"/>\n          </div>\n          <div class=\"storage\">保存</div>\n        </div>\n      </div>\n    </main>\n    <div class=\"mask\" ref=\"mask\" @click.self=\"cancelAddress\"></div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'place',\n  data () {\n    return {\n      aim: 0,\n      plan: '',\n      goal: '0',\n      sign: '',\n      truncation: 3,\n      address_quantity: 6,\n      detail_address: '',\n      choice_address: {\n        0: '省',\n        1: '市',\n        2: '区',\n        3: '街道'\n      },\n      address: [],\n      text: '',\n      test_receiving: [\n        {\n          receiver: '余俊雄',\n          id: 1,\n          phone: '13440069172',\n          detailed_address: '花园小区',\n          city_address: [\n            { 0: '四川省', 1: 51 },\n            { 0: '广安市', 1: 5116 },\n            { 0: '武胜县', 1: 511622 },\n            { 0: '乐善镇', 1: 511622014 }\n          ]\n        },\n        {\n          receiver: '余俊雄',\n          id: 2,\n          phone: '13440069172',\n          detailed_address: '花园小区',\n          city_address: [\n            { 0: '四川省', 1: 51 },\n            { 0: '广安市', 1: 5116 },\n            { 0: '武胜县', 1: 511622 },\n            { 0: '乐善镇', 1: 511622014 }\n          ]\n        },\n        {\n          receiver: '余俊雄',\n          id: 3,\n          phone: '13440069172',\n          detailed_address: '花园小区',\n          city_address: [\n            { 0: '四川省', 1: 51 },\n            { 0: '广安市', 1: 5116 },\n            { 0: '武胜县', 1: 511622 },\n            { 0: '乐善镇', 1: 511622014 }\n          ]\n        },\n        {\n          receiver: '余俊雄',\n          id: 4,\n          phone: '13440069172',\n          detailed_address: '花园小区',\n          city_address: [\n            { 0: '四川省', 1: 51 },\n            { 0: '广安市', 1: 5116 },\n            { 0: '武胜县', 1: 511622 },\n            { 0: '乐善镇', 1: 511622014 }\n          ]\n        },\n        {\n          receiver: '余俊雄',\n          id: 5,\n          phone: '13440069172',\n          detailed_address: '花园小区',\n          city_address: [\n            { 0: '四川省', 1: 51 },\n            { 0: '广安市', 1: 5116 },\n            { 0: '武胜县', 1: 511622 },\n            { 0: '乐善镇', 1: 511622014 }\n          ]\n        },\n        {\n          receiver: '余俊雄',\n          id: 6,\n          phone: '13440069172',\n          detailed_address: '花园小区',\n          city_address: [\n            { 0: '四川省', 1: 51 },\n            { 0: '广安市', 1: 5116 },\n            { 0: '武胜县', 1: 511622 },\n            { 0: '乐善镇', 1: 511622014 }\n          ]\n        }\n      ]\n    }\n  },\n  methods: {\n    // 取消地址选择\n    cancelAddress () {\n      this.$refs.choice.style = 'display: none'\n    },\n    // 详细地址选择\n    detailAddress (id, name) {\n      // 把 id 详细地址id标注到所对应的类上\n      this.$refs[`address${parseInt(this.goal)}`][0].setAttribute('data-id', id)\n      this.address[parseInt(this.goal)] = { 0: name, 1: parseInt(id) }\n      // 发起请求获得详细地址id的数据\n      if (parseInt(this.goal) < 3) {\n        this.getAddress(id, 1)\n      }\n      // 需要把后面所有省/市/街区的id去掉\n      for (let i = 0; i < 4; i++) {\n        if (i > parseInt(this.goal)) {\n          this.$refs[`address${i}`][0].removeAttribute('data-id')\n          delete this.address[i]\n        }\n      }\n      if (parseInt(this.goal) >= 3) {\n        this.$refs.choice.style = 'display: none'\n        this.sign = id\n      } else {\n        this.goal = String(parseInt(this.goal) + 1)\n      }\n    },\n    // 省/市/区/街道选择\n    choices (index) {\n      let id = this.$refs[`address${index}`][0].getAttribute('data-id')\n      // 如果当前点击元素的 id 是空的\n      if (id) {\n        this.goal = index\n        this.getAddress(id, 0)\n        this.sign = id\n      } else if (index !== '0') {\n        let fatherId = this.$refs[`address${index - 1}`][0].getAttribute('data-id') // 点击元素的上级id\n        if (fatherId) {\n          this.goal = index\n          this.getAddress(fatherId, 1)\n        }\n      }\n    },\n    // 点击选择地址\n    choiceAddress () {\n      this.$refs.choice.style = 'display: block'\n      if (!this.detail_address) {\n        this.getAddress(0, 1)\n      }\n    },\n    // 新增地址\n    addAddress () {\n      this.$refs.addAddress.style = 'display: block'\n      this.$refs.mask.style = 'display: block'\n    },\n    // 关闭新增地址\n    closeAddAddress () {\n      this.$refs.addAddress.style = 'display: none'\n      this.$refs.mask.style = 'dis