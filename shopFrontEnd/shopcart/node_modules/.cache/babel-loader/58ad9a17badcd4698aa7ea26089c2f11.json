ed","id","substr","$https","post","then","res","code","getOrderInformation","catch","console","log","err","$message","type","message","FullPayment","$router","push","path","query","ChoiceShop","ModifyOrder","num","CancellationOrder","$confirm","confirmButtonText","cancelButtonText","ImmediatePayment","shop_cart_info","created","$route","watch","handler","newVal","oldVal","deep"],"sourceRoot":"","sources":["/Users/yujunxiong/Desktop/shopFrontEnd/shopcart/src/components/user/OrderInformation.vue"],"sourcesContent":["<template>\n  <div>\n    <table>\n      <colgroup>\n        <col style=\"width: 38%;\">\n        <col style=\"width: 10%;\">\n        <col style=\"width: 5%;\">\n        <col style=\"width: 12px;\">\n        <col style=\"width: 12%;\">\n        <col style=\"width: 11%;\">\n        <col style=\"width: 12%;\">\n      </colgroup>\n      <thead>\n        <tr>\n          <th>宝贝</th>\n          <th>单价</th>\n          <th>数量</th>\n          <th>商品操作</th>\n          <th>实付款</th>\n          <th>交易状态</th>\n          <th>交易操作</th>\n        </tr>\n        <tr class=\"sep-row\">\n          <td colspan=\"4\">\n            <div class=\"row-left\" v-if=\"name==='payoff'||name==='receiving'\">\n              <div>\n                <input type=\"checkbox\" v-model=\"select_all\" @change=\"ChoiceShop(1)\">\n                <strong>全选</strong>\n              </div>\n              <span @click=\"FullPayment\" v-if=\"name==='payoff'\">合并付款</span>\n              <span @click=\"FullPayment\" v-if=\"name==='payoff'\">合并代付</span>\n              <span v-if=\"name==='receiving'\" style=\"margin-right: 54px;\" @click=\"ConfirmReceipt\">批量确认收货</span>\n            </div>\n          </td>\n          <td colspan=\"3\">\n            <div class=\"row-right\">\n              <span style=\"margin-right: 15px;\">上一页</span>\n              <span>下一页</span>\n            </div>\n          </td>\n        </tr>\n      </thead>\n      <tbody v-for=\"(item,index) in order_info\">\n        <tr>\n          <td colspan=\"7\" class=\"interval\"></td>\n        </tr>\n        <tr class=\"title\">\n          <td>\n            <div class=\"bought-wrapper\">\n              <div>\n                <input type=\"checkbox\" v-model=\"item.content[0].selected\" @change=\"ChoiceShop(0)\" v-if=\"name==='payoff'||name==='receiving'\">\n                <span>{{item.content[0].time}}</span>\n              </div>\n              <span>订单号: <i>2646139393277811146</i></span>\n            </div>\n          </td>\n          <td colspan=\"2\">\n            <span class=\"store-name\">{{item.store}}</span>\n          </td>\n          <td colspan=\"4\"></td>\n        </tr>\n        <tr class=\"container\">\n          <td style=\"border-left: 1px solid #DAF3FF;\" class=\"bottom-out\">\n            <div class=\"ml-mod\">\n              <img :src=\"item.content[0].goods_image\" alt=\"\" class=\"image\"/>\n              <div class=\"shop-container\">\n                <span class=\"shop-title\">{{item.content[0].title}}</span>\n                <div class=\"shop-opt\">\n                  <span v-for=\"value in item.content[0].opts\">{{value.type}}：{{value.content}}</span>\n                </div>\n                <img src=\"@/assets/verygoods/baozhang.png\" alt=\"\" />\n              </div>\n            </div>\n          </td>\n          <td class=\"bottom-out\">\n            <div class=\"unit-price\">\n              ¥ {{item.content[0].price}}.00\n            </div>\n          </td>\n          <td class=\"bottom-out\">\n            <div class=\"unit-price\">\n              {{item.content[0].num}}\n            </div>\n          </td>\n          <td class=\"bottom-out\">\n            <!-- 已签收 -->\n            <div class=\"shop-operation\" v-if=\"name==='sign'\">\n              <span>申请售后</span>\n              <span>退运保险</span>\n            </div>\n            <!-- 待付款 -->\n            <div class=\"shop-operation\" v-if=\"name==='payoff'\">\n              <span>违规举报</span>\n              <span>退运保险</span>\n            </div>\n            <!-- 待发货 -->\n            <div class=\"shop-operation\" v-if=\"name==='delivery'\">\n              <span>退款/退货</span>\n              <span>投诉商家</span>\n              <span>退运保险</span>\n            </div>\n            <!-- 待收货 -->\n            <div class=\"shop-operation\" v-if=\"name==='receiving'\">\n              <span>退款/退换货</span>\n              <span>投诉商家</span>\n              <span>运费险已出单</span>\n            </div>\n          </td>\n          <td class=\"undecided\">\n            <div class=\"total-price\">\n              <i>¥ {{item.content[0].price*item.content[0].num}}.00</i>\n              <span>(包含运费：¥0.00)</span>\n            </div>\n          </td>\n          <td class=\"undecided\">\n            <!-- 已签收 -->\n            <div class=\"shop-operation\" v-if=\"name==='sign'\">\n              <span>交易成功</span>\n              <span>订单详情</span>\n            </div>\n            <!-- 待付款 -->\n            <div class=\"shop-operation\" v-if=\"name==='payoff'\">\n              <span>等待卖家付款</span>\n              <span>订单详情</span>\n            </div>\n            <!-- 待发货 -->\n            <div class=\"shop-operation\" v-if=\"name==='delivery'\">\n              <span>买家已付款</span>\n              <span>订单详情</span>\n            </div>\n            <!-- 待收货 -->\n            <div class=\"shop-operation\" v-if=\"name==='receiving'\">\n              <span>卖家已发货</span>\n              <span>订单详情</span>\n              <span>查看物流</span>\n            </div>\n          </td>\n          <td class=\"undecided\">\n            <!-- 已签收 -->\n            <div class=\"transaction-operation\" v-if=\"name==='sign'\">\n              <span>申请开票</span>\n            </div>\n            <!-- 待付款 -->\n            <div class=\"transaction-operation\" v-if=\"name==='payoff'\">\n              <el-button type=\"success\" @click=\"ImmediatePayment(item.content[0].id)\">立即付款</el-button>\n              <span>找朋友帮忙付</span>\n              <span @click=\"CancellationOrder(item.content[0].id)\">取消订单</span>\n              <span @click=\"ModifyOrder(item.content[0].id, item.content[0].num)\">修改订单</span>\n            </div>\n            <!-- 待发货 -->\n            <div class=\"transaction-operation\" v-if=\"name==='delivery'\">\n              <span>修改订单</span>\n              <span>申请开票</span>\n            </div>\n            <!-- 待收货 -->\n            <div class=\"transaction-operation\" v-if=\"name==='receiving'\">\n              <span><i class=\"iconfont\">&#xe6b5;</i>还剩3天2时</span>\n              <el-button type=\"success\" @click=\"ConfirmReceipt(item.content[0].id)\">确认收货</el-button>\n              <span>申请开票</span>\n            </div>\n          </td>\n        </tr>\n        <tr class=\"insurance\">\n          <td style=\"border-left: 1px solid #DAF3FF;\" class=\"bottom-out\">\n            保险服务\n          </td>\n          <td class=\"bottom-out\">\n            ¥ 0.00\n          </td>\n          <td class=\"bottom-out\"></td>\n          <td class=\"bottom-out\"></td>\n          <td class=\"head-less\"></td>\n          <td class=\"head-less\"></td>\n          <td class=\"head-less\"></td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'test',\n  data () {\n    return {\n      // 全选按钮\n      select_all: false,\n      info: {\n        payoff: 2,\n        delivery: 3,\n        receiving: 6,\n        sign: 5\n      },\n      name: '',\n      opt: {\n        0: '选择颜色',\n        1: '选择尺码',\n        2: '选择系列',\n        3: '选择版本',\n        4: '选择规格',\n        5: '选择类别',\n        6: '选择性别'\n      },\n      order_info: ''\n    }\n  },\n  methods: {\n    // 批量确认收货\n    ConfirmReceipt (shopcart) {\n      let idlist = ''\n      for (let i = 0; i < this.order_info.length; i++) {\n        if (this.order_info[i].content[0].selected === true) {\n          let id = this.order_info[i].content[0].id\n          idlist += id + '.'\n          idlist = idlist.substr(0, idlist.length - 1)\n        }\n      }\n      if (typeof shopcart === 'number') {\n        idlist = shopcart + ''\n      }\n      if (idlist !== '') {\n        this.$https.post('user/confirm_receipt/', { id: idlist }).then(res => {\n          if (res.data.code === 200) {\n            this.getOrderInformation()\n          }\n        }).catch(err => {\n          console.log(err)\n        })\n      } else {\n        this.$message({\n          type: 'info',\n          message: '请选择商品'\n        })\n      }\n    },\n    // 批量付款\n    FullPayment () {\n      let idlist = ''\n      for (let i = 0; i < this.order_info.length; i++) {\n        if (this.order_info[i].content[0].selected === true) {\n          let id = this.order_info[i].content[0].id\n          idlist += id + '.'\n        }\n      }\n      if (idlist !== '') {\n        this.$router.push({\n          path: '/sure/payment',\n          query: {\n            id: idlist.substr(0, idlist.length - 1)\n          }\n        })\n      } else {\n        this.$message({\n          type: 'info',\n          message: '请选择商品'\n        })\n      }\n    },\n    // 商品选择\n    ChoiceShop (type) {\n      if (type) {\n        if (this.select_all) {\n          for (let i = 0; i < this.order_info.length; i++) {\n            this.order_info[i].content[0].selected = true\n          }\n        } else {\n          for (let i = 0; i < this.order_info.length; i++) {\n            this.order_info[i].content[0].selected = false\n          }\n        }\n      } else {\n        for (let i = 0; i < this.order_info.length; i++) {\n          if (this.order_info[i].content[0].selected === true) {\n            this.select_all = true\n          } else {\n            this.select_all = false\n            break\n          }\n        }\n      }\n    },\n    // 修改订单\n    ModifyOrder (id, num) {\n      this.$router.push({\n        path: '/payment',\n        query: {\n          id: id,\n          num: num,\n          type: 1\n        }\n      })\n    },\n    // 取消订单\n    CancellationOrder (id) {\n      this.$confirm('你确定要取消该订单吗？, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        })\n        // 删除数据请求\n        this.$https.post('user/delete_shop_cart/', { id: id }).then(res => {\n          if (res.data.code === 200) {\n            this.getOrderInformation()\n          }\n        }).catch(err => {\n          console.log(err)\n        })\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        })\n      })\n    },\n    // 立即付款\n    ImmediatePayment (id) {\n      this.$router.push({\n        path: '/sure/payment',\n        query: {\n          id: id\n        }\n      })\n    },\n    getOrderInformation () {\n      let type = this.info[this.name]\n      this.$https.post('user/shopcart/', { type: type }).then(res => {\n        if (res.data.code === 200) {\n          this.order_info = res.data.data.shop_cart_info\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    }\n  },\n  created () {\n    this.getOrderInformation()\n    this.name = this.$route.name\n  },\n  watch: {\n    name: {\n      handler (newVal, oldVal) {\n        this.getOrderInformation()\n      }\n    },\n    $route: {\n      handler (newVal, oldVal) {\n        this.name = newVal.name\n      },\n      // 深度观察监听\n      deep: true\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\ntable {\n  width: 100%;\n}\nthead {\n  tr {\n    border-top: 1px solid #e0e0e0;\n  }\n}\n.interval {\n  height: 15px;\n  background: #fff;\n  border: 0;\n}\nth,td {\n  font-size: 12px;\n  height: 39px;\n  text-align: center;\n  background: #f5f5f5;\n  color: #666;\n  font-weight: 400;\n  border-left: 1px solid #e0e0e0;\n  border-right: 1px solid #e0e0e0;\n}\n.sep-row {\n  height: 39px;\n  td {\n    background: #fff;\n    border-left: 0;\n    border-right: 0;\n 