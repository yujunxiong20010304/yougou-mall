AK;AAAA,iBAAEtB,gBAAOO,KAAP,EAAcM,CAAd,CAAF;AAAA;AAAqC,OAApE,EAAmD,GAAnD,EAA2D;AAAA;AAA3D,QAA2DgB,WAA3D,CACI,CALN,CAWI,EALJzC,oBAA8D,MAA9D,eAAwB,MAACK,iBAAEmB,KAAK,CAACS,KAAN,GAAcT,KAAK,CAACW,GAAtB,CAAD,GAA4B,KAApD,EAAuD;AAAA;AAAvD,OAKI,EAJJnC,oBAGM,KAHN,eAGM,CAFJ0C,WAEI,EADJ1C,oBAAiD,MAAjD,EAAiD;AAA1CkC,eAAK;AAAA,iBAAEtB,wBAAeY,KAAK,CAAC1B,EAArB,CAAF;AAAA;AAAqC,OAAjD,EAAwC,IAAxC,EAA0C;AAAA;AAA1C,QAA0C6C,WAA1C,CACI,CAHN,CAII,CAbN,CAcG,CAhCL;KAgCK,CAhCL;;AAAA,KAiCG,EAlCL,CAmCG,CAvCL;GAuCK,CAvCL;;AAAA,GAwCG,EAzCL,CA4DI,EAlBJN,iaAkBI,EARJ7B,aAAyBoC,qBAAzB,CAQI,EAPJ5C,oBAMM,KANN,eAMM,CALJQ,aAAkHC,sBAAlH,EAAkH;gBAA5FH,aAA4F;;aAA5FA,gBAAOI;MAAqF;AAAnFT,SAAwC,EAAxC;AAAA;AAAA;AAAA,KAAmF;AAAzCU,YAAM;AAAA,aAAEC,sBAAY,CAAZ,CAAF;AAAA;AAAmC,GAAlH;sBAAkG;AAAA,aAAE,aAAF;AAAA;;;;GAAlG;;AAAA,mBAKI,EAJJiC,WAII,EAHJC,WAGI,EAFJC,WAEI,EADJC,WACI,CANN,CAOI,CAlFN","names":["_imports_0","_imports_1","_imports_2","id","class","_createElementVNode","style","src","alt","_createElementBlock","_toDisplayString","$data","_hoisted_4","_createVNode","_component_el_checkbox","$event","onChange","$options","_hoisted_8","_hoisted_9","_hoisted_10","_Fragment","_renderList","item","index","select","fill","store","content","value","h","selected","goods_image","title","_hoisted_20","opts","i","type","price","onClick","num","_hoisted_26","_createCommentVNode","autocomplete","onBlur","onkeypress","_hoisted_28","_hoisted_31","_hoisted_32","_component_el_divider","_hoisted_35","_hoisted_36","_hoisted_37","_hoisted_38"],"sourceRoot":"","sources":["/Users/yujunxiong/Desktop/shopFrontEnd/shopcart/src/components/user/ShopCart.vue"],"sourcesContent":["<template>\n  <div id=\"shop-cart\">\n    <div class=\"title\">\n      <h4>购物车（全部{{shop_num}}）</h4>\n      <div class=\"all-price\">\n        <span>已选商品（不含运费）</span>\n        <i>{{total_price}}.00 ¥</i>\n        <div class=\"settlement\" :style=\"[total_price === 0?'':'backgroundColor: #FF5001']\">结算</div>\n      </div>\n    </div>\n    <div class=\"cart-table-th\">\n      <div class=\"thm-1 box\">\n        <el-checkbox v-model=\"checked\" style=\"font-weight: 700;color: #3c3c3c;\" @change=\"changeSelect(1)\">全选</el-checkbox>\n        <span>商品信息</span>\n      </div>\n      <div style=\"width: 170px;\"></div>\n      <div class=\"thm-2 box\">\n        <span>单价</span>\n        <span>数量</span>\n        <span>金额</span>\n        <span>操作</span>\n      </div>\n    </div>\n    <ul class=\"shop-list\">\n      <li v-for=\"(item, index) in shop_cart\">\n        <div class=\"store-title\">\n          <el-checkbox v-model=\"item.select\" text-color=\"#fff\" fill=\"#fff\" @change=\"changeSelect(2, index)\">店铺：<i>{{item.store}}</i></el-checkbox>\n        </div>\n        <ul class=\"store-shop\" style=\"flex-direction: column;\">\n          <li v-for=\"(value,h) in item.content\" class=\"shop\">\n            <div class=\"container-1\">\n              <div class=\"shop-img\">\n                <el-checkbox v-model=\"value.selected\" @change=\"changeSelect(3, index, value.id)\"></el-checkbox>\n                <img :src=\"value.goods_image\" alt=\"\"/>\n              </div>\n              <div class=\"shop-present\">\n                <span>{{value.title}}</span>\n                <div class=\"shop-promise\">\n                  <img src=\"@/assets/verygoods/xcard.png\" alt=\"\" />\n                  <img src=\"@/assets/verygoods/sun.png\" alt=\"\" />\n                  <img src=\"@/assets/verygoods/seven.png\" alt=\"\" />\n                </div>\n              </div>\n            </div>\n            <div class=\"opt\">\n              <span v-for=\"i in value.opts\" style=\"padding-right: 12px;\">{{opt[i.type]}}:{{i.content}}</span>\n            </div>\n            <div class=\"container-2\">\n              <span class=\"price\">¥{{value.price}}</span>\n              <div class=\"num\">\n                <button class=\"iconfont\" @click=\"value.num++,totalPrice()\">&#xe654;</button>\n                <!--<input type=\"text\" alt=\"\" v-model=\"value.num\"/>-->\n                <input v-model=\"value.num\" type=\"number\" autocomplete=\"off\" @blur=\"value.num=value.num===0?1:value.num, totalPrice()\" onkeypress=\"return (/[\\d]/.test(String.fromCharCode(event.keyCode)))\" style=\"ime-mode:Disabled\">\n                <button class=\"iconfont\" @click=\"reduce(index, h)\">&#xe656;</button>\n              </div>\n              <span class=\"all-price\">¥{{value.price * value.num}}.00</span>\n              <div class=\"handle\">\n                <span>移入收藏夹</span>\n                <span @click=\"deleteShopCart(value.id)\">删除</span>\n              </div>\n            </div>\n          </li>\n        </ul>\n      </li>\n    </ul>\n    <!--<a-affix :offset-bottom=\"78\" @change=\"change\">\n      <el-divider></el-divider>\n      <div class=\"tail\">\n        <el-checkbox v-model=\"checked\" style=\"font-weight: 700;color: #3c3c3c;\" @change=\"changeSelect(1)\">全选</el-checkbox>\n        <span>删除</span>\n        <span>已选商品<i>1</i>件</span>\n        <span>合计（不含运费）</span>\n        <span>¥ 109.00</span>\n      </div>\n    </a-affix>-->\n    <el-divider></el-divider>\n    <div class=\"tail\">\n      <el-checkbox v-model=\"checked\" style=\"font-weight: 700;color: #3c3c3c;\" @change=\"changeSelect(1)\">全选</el-checkbox>\n      <span>删除</span>\n      <span>已选商品<i>1</i>件</span>\n      <span>合计（不含运费）</span>\n      <span>¥ 109.00</span>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'ShopCart',\n  data () {\n    return {\n      shop_cart: [],\n      opt: {\n        0: '选择颜色',\n        1: '选择尺码',\n        2: '选择系列',\n        3: '选择版本',\n        4: '选择规格',\n        5: '选择类别',\n        6: '选择性别'\n      },\n      checked: false,\n      total_price: 0,\n      shop_num: 0\n    }\n  },\n  // 请求购物车信息\n  methods: {\n    handleScroll () {\n      let scrollTop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop\n      console.log(scrollTop, '滚动距离')\n    },\n    totalPrice () {\n      this.total_price = 0\n      this.shop_num = 0\n      for (let store of this.shop_cart) {\n        for (let shop of store.content) {\n          this.shop_num++\n          if (shop.selected === true) {\n            this.total_price = shop.num * shop.price + this.total_price\n          }\n        }\n      }\n    },\n    deleteShopCart (id) {\n      this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        })\n        // 删除数据请求\n        this.$https.post('user/delete_shop_cart/', { id: id }).then(res => {\n          if (res.data.code === 200) {\n            this.getShopCart()\n          }\n        }).catch(err => {\n          console.log(err)\n        })\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        })\n      })\n    },\n    // 商品数量减少\n    reduce (index, i) {\n      this.totalPrice()\n      let num = this.shop_cart[index].content[i].num\n      if (num > 1) {\n        this.shop_cart[index].content[i].num--\n      }\n    },\n    // 共有单选多选操作\n    changeAll () {\n      let length = this.shop_cart.length\n      for (let i = 0; i < length; i++) {\n        if (this.shop_cart[i].select === true) {\n          this.checked = true\n        } else {\n          this.checked = false\n          break\n        }\n      }\n    },\n    // 单选多选操作\n    changeSelect (type, index, id) {\n      if (type === 1) {\n        let length = this.shop_cart.length\n        for (let i = 0; i < length; i++) {\n          this.shop_cart[i].select = this.checked\n          for (let h of this.shop_cart[i].content) {\n            h.selected = this.checked\n          }\n        }\n      }\n      if (type === 2) {\n        let judge = this.shop_cart[index].select\n        for (let i of this.shop_cart[index].content) {\n          i.selected = judge\n        }\n        this.changeAll()\n      }\n      if (type === 3) {\n        for (let i of this.shop_cart[index].content) {\n          if (i.selected === true) {\n            this.shop_cart[index].select = true\n          } else {\n            this.shop_cart[index].select = false\n            break\n          }\n        }\n        this.changeAll()\n      }\n      this.totalPrice()\n    },\n    getShopCart () {\n      this.$https.post('user/shopcart/').then(res => {\n        this.shop_cart = res.data.data.shop_cart_info\n        this.totalPrice()\n      }).catch(err => {\n        console.log(err)\n      })\n    }\n  },\n  created () {\n    this.getShopCart()\n  },\n  mounted () {\n    document.querySelector('#shop-cart').addEventListener('scroll', this.handleScroll) // 监听页面滚动\n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n#shop-cart {\n  width: 100%;\n  /*border-top-right-radius: 25px;\n  border-top-left-radius: 25px;*/\n  box-shadow:0 15px 35px rgba(255,255,255,0.2),0 2px 5px rgba(0,0,0,0.2);\n  border: 1px solid #ccc;\n  // padding-bottom: 45px;\n  border-radius: 25px;\n  overflow: hidden;\n  .shop-list {\n    margin: 0;\n    padding: 0;\n  }\n  .title {\n    width: 100%;\n    padding: 0 18px;\n    height: 73px;\n    display: flex;\n    background-color: #fff;\n    border-top-right-radius: 25px;\n    border-top-left-radius: 25px;\n    align-items: center;\n    justify-content: space-between;\n    border-bottom: 1px solid #ccc;\n    h4 {\n      font-size: 18px;\n      color: #000;\n      font-weight: 800;\n      margin: 0;\n    }\n    .all-price {\n      height: 100%;\n      display: flex;\n      align-items: center;\n      justify-content: space-between;\n      span {\n        font-size: 14px;\n        color: #000;\n        font-weight: 450;\n      }\n      i {\n        font-style: normal;\n        font-family: Verdana,Arial,serif;\n        font-weight: 500;\n        font-size: 22px;\n        color: #ff5000;\n        margin: 0 10px;\n      }\n      .settlement {\n 