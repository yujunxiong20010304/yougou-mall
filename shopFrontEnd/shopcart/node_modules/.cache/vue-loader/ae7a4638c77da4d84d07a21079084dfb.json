:["<template>\n  <div id=\"sidebar\" :style=\"{ height: height + 'px' }\">\n     <el-menu\n    default-active=\"start\"\n    class=\"el-menu-vertical-demo\"\n    :collapse=\"isCollapse\"\n    @select=\"handleSelect\"\n    >\n      <el-menu-item index=\"start\" @click=\"change('yougou')\">\n        <el-icon class=\"iconfont\">&#xe6a4;</el-icon>\n        <template #title>我的优购</template>\n      </el-menu-item>\n      <el-sub-menu index=\"1\">\n        <template #title>\n          <el-icon class=\"iconfont\">&#xe693;</el-icon>\n          <span>我的订单</span>\n        </template>\n          <el-menu-item index=\"1-1\" @click=\"change('shopcart')\">购物车</el-menu-item>\n          <el-menu-item index=\"1-2\" @click=\"change('paid')\">待付款</el-menu-item>\n          <el-menu-item index=\"1-3\" @click=\"change('toshipped')\">待发货</el-menu-item>\n          <el-menu-item index=\"1-4\" @click=\"change('received')\">待收货</el-menu-item>\n          <el-menu-item index=\"1-5\" @click=\"change('toevaluated')\">待评价</el-menu-item>\n          <el-menu-item index=\"1-6\" @click=\"change('sign')\">已签收</el-menu-item>\n      </el-sub-menu>\n      <el-sub-menu index=\"2\">\n        <template #title>\n          <el-icon class=\"iconfont\">&#xe694;</el-icon>\n          <span>账号设置</span>\n        </template>\n          <el-menu-item index=\"2-1\" @click=\"change('accountinformation')\">账号信息</el-menu-item>\n          <el-menu-item index=\"2-2\" @click=\"change\">收货信息</el-menu-item>\n      </el-sub-menu>\n\n      <el-sub-menu index=\"3\">\n        <template #title>\n          <el-icon class=\"iconfont\">&#xe697;</el-icon>\n          <span>我的关注</span>\n        </template>\n          <el-menu-item index=\"3-1\" @click=\"change\">关注店铺</el-menu-item>\n          <el-menu-item index=\"3-2\" @click=\"change\">关注商品</el-menu-item>\n      </el-sub-menu>\n    </el-menu>\n  </div>\n</template>\n<script>\nimport emitter from '@/utils/event'\n\nexport default {\n  name: 'sidebar',\n  data () {\n    return {\n      isCollapse: true,\n      height: ''\n    }\n  },\n  methods: {\n    change (name) {\n      emitter.emit('change', { aim: name })\n    },\n    handleSelect (index, indexPath) {\n      console.log(index, indexPath)\n    }\n  },\n  mounted () {\n    emitter.on('stretch', (e) => {\n      this.isCollapse = e.aim\n    })\n    // 设置窗口大小\n    this.height = document.documentElement.clientHeight\n    window.onresize = () => {\n      return (() => {\n        this.height = document.documentElement.clientHeight\n      })()\n    }\n  },\n  watch: {\n    height: {\n      handler (newVal, oldVal) {\n        emitter.emit('height', { aim: this.height })\n      }\n    }\n  }\n}\n</script>\n\n<style>\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 200px;\n  height: 100%;\n}\n.el-menu--collapse {\n  height: 100%;\n}\n</style>\n"]}]}